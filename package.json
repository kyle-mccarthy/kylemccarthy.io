{
  "name": "nest-next-app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Kyle McCarthy",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:kyle-mccarthy/nest-next-starter.git"
  },
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build:clean": "rm -rf .next",
    "build:server": "npx tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server --config-file ./.babelrc-server --extensions \".js\"",
    "build:ui": "npx next build",
    "build": "yarn build:clean && yarn build:server && yarn build:ui",
    "start": "NODE_ENV=production node --require reflect-metadata .next/production-server/main.js",
    "test": "npx jest",
    "test:e2e": "jest --config ./jest-e2e.json"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@nestjs/common": "^5.3.9",
    "@nestjs/core": "^5.3.10",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/testing": "^5.3.11",
    "@nestjs/typeorm": "^5.2.2",
    "bcrypt": "^3.0.2",
    "class-validator": "^0.9.1",
    "dotenv": "^6.1.0",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "emotion-theming": "^9.2.9",
    "fastify": "^1.12.1",
    "fastify-formbody": "^2.0.1",
    "jest": "^23.6.0",
    "joi": "^13.7.0",
    "nest-next": "^0.1.0",
    "next": "^7.0.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.5.0",
    "radium": "^0.25.0",
    "react": "^16.5.2",
    "react-burger-menu": "^2.5.3",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.2",
    "ts-jest": "^23.10.4",
    "tslint-react": "^3.6.0",
    "typeorm": "^0.2.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.1.6",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.5",
    "@types/joi": "^13.6.0",
    "@types/mocha": "^5.2.5",
    "@types/next": "^6.1.8",
    "@types/passport-jwt": "^3.0.1",
    "@types/radium": "^0.24.2",
    "@types/react-burger-menu": "^2.2.1",
    "@types/supertest": "^2.0.6",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "next-images": "^1.0.1",
    "sql.js": "^0.5.0",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "coverageDirectory": "../coverage",
    "preset": "ts-jest/presets/js-with-ts",
    "testMatch": null
  }
}
